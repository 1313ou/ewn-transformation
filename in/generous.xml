	<!-- GENEROUS -->

		<!-- generous -->
		<Synset id="ewn-01114727-a" ili="i6102" partOfSpeech="a" dc:subject="adj.all" lexicalized="true">
			<Definition>willing to give and share unstintingly</Definition>
			<Example>"a generous donation"</Example>
		</Synset>
		
		<!-- similar: freehearted, benevolent -->
		<Synset id="ewn-01115023-s" ili="i6103" partOfSpeech="s" dc:subject="adj.all" lexicalized="true">
			<Definition>generous in providing aid to others</Definition>
			<SynsetRelation relType="similar" target="ewn-01114727-a" />
			<!-- generous -->
		</Synset>
		
		<!-- similar: bounteous, openhanded, freehanded, bighearted, big, liberal, giving, bountiful, handsome -->
		<Synset id="ewn-01115129-s" ili="i6104" partOfSpeech="s" dc:subject="adj.all" lexicalized="true">
			<Definition>given or giving freely</Definition>
			<SynsetRelation relType="similar" target="ewn-01114727-a" />
			<!-- generous -->
			<Example>"was a big tipper"</Example>
			<Example>"the bounteous goodness of God"</Example>
			<Example>"bountiful compliments"</Example>
			<Example>"a freehanded host"</Example>
			<Example>"a handsome allowance"</Example>
			<Example>"Saturday's child is loving and giving"</Example>
			<Example>"a liberal backer of the arts"</Example>
			<Example>"a munificent gift"</Example>
			<Example>"her fond and openhanded grandfather"</Example>
		</Synset>
		
		<!-- similar: unsparing, unstinting, too-generous, lavish, unstinted, overgenerous, munificent -->
